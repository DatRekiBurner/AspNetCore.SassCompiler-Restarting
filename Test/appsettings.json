{
    "webOptimizer": {
        "enableCaching": false,
        "enableMemoryCache": false,
        "enableDiskCache": false,
        "cacheDirectory": "/var/temp/weboptimizercache",
        "enableTagHelperBundling": true,
        "cdnUrl": "https://my-cdn.com/",
        "allowEmptyBundle": false
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "AllowedHosts": "*"
}
